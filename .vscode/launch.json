    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack://src/*": "${webRoot}/*"
            }
        },
        {
            "name": "Debug Node.js (server.js)",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "/Users/dheerajdoppalapudi/.nvm/versions/node/v20.18.3/bin/node",
            "program": "${workspaceFolder}/fileRetrival-backend/server.js",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}